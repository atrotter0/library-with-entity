@using Library.Models
@model Library.Models.Patron

@{
  Layout = "_Layout";
}

<div class="container">
    <h2>@Html.DisplayFor(model => model.FirstName) @Html.DisplayFor(model => model.LastName)</h2>
    <h4>Books:</h4>
    <ul>
        @foreach (var book in ViewBag.BookList)
        {
            <li>@book.BookTitle</li>
        }
    </ul>
    <hr />
    <p>@Html.ActionLink("Back to Patron List", "Index")</p>
</div>